# platform :ios, '9.0'

target 'DailyPlanner' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DailyPlanner
  pod 'SnapKit', '~> 4.0'
  pod 'JTAppleCalendar', '~> 7.0'
  pod 'FSCalendar'

end
